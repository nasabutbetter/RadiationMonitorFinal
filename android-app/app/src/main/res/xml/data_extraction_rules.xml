<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfSdkLessThan="31">
        <include domain="file" path="."/>
        <exclude domain="no_backup"/>
    </cloud-backup>
    <device-transfer>
        <include domain="file" path="."/>
        <exclude domain="no_backup"/>
    </device-transfer>
</data-extraction-rules>
